<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bastosbf.market.search.server.model.PriceSuggestion"
		table="price_suggestion">
		<id name="id" column="id">
			<generator class="identity" />
		</id>
		<many-to-one name="market"
			class="com.bastosbf.market.search.server.model.Market" fetch="select" />
		<many-to-one name="product"
			class="com.bastosbf.market.search.server.model.Product" fetch="select" />
		<property name="price" />
		<property name="date" />
	</class>
</hibernate-mapping>
