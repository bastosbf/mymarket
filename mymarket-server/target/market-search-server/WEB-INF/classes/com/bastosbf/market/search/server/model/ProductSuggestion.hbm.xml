<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bastosbf.market.search.server.model.ProductSuggestion"
		table="product_suggestion">
		<id name="id" column="id">
			<generator class="identity" />
		</id>
		<many-to-one name="market"
			class="com.bastosbf.market.search.server.model.Market" fetch="select" />
		<property name="barcode" />
		<property name="name" />
		<property name="brand" />
		<property name="price" />
	</class>
</hibernate-mapping>